oardPos === \"O\" && !winCheckArray.includes(\"X\")){\n        hasWon = true \n      }\n    })\n    return hasWon\n  }\n\n  handleClick = (cellIndex) => {\n    const { board, currentToken, turn } = this.state; \n\n    if (board[cellIndex] !== \"\"){\n      return \n    }\n\n    const cloneBoard = [...board]\n    cloneBoard[cellIndex] = currentToken;\n    \n    if (turn >= 4 && this.checkForWinner(cloneBoard)){\n    this.setState(() => ({\n        board: cloneBoard,\n        canPlay: false \n    }))\n    return \n  }\n\n  this.setState(() => ({\n    board: cloneBoard, \n    currentToken: currentToken === \"X\" ? \"O\" : \"X\", \n    turn: turn + 1 \n  }))\n  }\n\n  render(){\n    const {board, canPlay, currentToken } = this.state \n  return (\n    <div className=\"App\">\n      <header className=\"App-header\">\n        Tic-Tac-Toe \n      </header>\n    {!canPlay ? <Message winnerToken ={currentToken}/> : null}\n    <Board \n      gameBoard={board} \n      canPlay={canPlay}\n      handleClick={this.handleClick}\n    />\n    </div>\n  );\n}\n}\n\nexport default App;\n","/mnt/c/Users/wendy/linked-list-lab-g-416/tic-tac-toe/src/reportWebVitals.js",[],"/mnt/c/Users/wendy/linked-list-lab-g-416/tic-tac-toe/src/Message.js",[],{"ruleId":"17","severity":1,"message":"18","line":32,"column":39,"nodeType":"19","messageId":"20","endLine":32,"endColumn":49},{"ruleId":"21","severity":2,"message":"22","line":35,"column":35,"nodeType":"19","messageId":"23","endLine":35,"endColumn":41},"no-unused-vars","'thirdIndex' is assigned a value but never used.","Identifier","unusedVar","no-undef","'tIndex' is not defined.","undef"]